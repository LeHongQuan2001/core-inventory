{
    "name": "frontend",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "buildwithstats": "ng build --source-map --configuration=production --stats-json --build-optimizer --vendor-chunk=true --aot --base-href=/ --output-path=../backend/html/dist/stats",
        "analyze": "webpack-bundle-analyzer --port 9901 ../backend/html/dist/stats/stats.json",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "start": "npm run hash:i18n && ng serve",
        "hash:i18n": "node scripts/hash-translations.js",
        "html-minifier": "node scripts/html-minifier.js",
        "build-development": "npm run hash:i18n && ng build --configuration=development  --output-path=../backend/html/dist/dev && npm run html-minifier",
        "build-staging": "npm run hash:i18n && ng build --configuration=staging  --output-path=../backend/html/dist/staging && npm run html-minifier",
        "build-production": "npm run hash:i18n && ng build --configuration=production --build-optimizer --aot --base-href=/ --output-path=../backend/html/dist/prod && npm run html-minifier"
    },
    "private": true,
